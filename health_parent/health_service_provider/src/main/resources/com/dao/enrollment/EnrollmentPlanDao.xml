<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
		"http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.dao.enroller.EnrollmentPlanDao">
    <!--新增-->
	<insert id="add" parameterType="com.pojo.EnrollmentPlan">
    
        insert into enroll_plan(code,name,sex,helpCode,remark,attention)
        values
	        (#{code},#{name},#{sex},#{helpCode},#{remark},#{attention})
    </insert>
	
	
	<select id="query" resultType="com.pojo.EnrollmentPlan">
      select * from enroll_plan
	</select>
	
	
	
	<!--编辑-->
	<update id="edit" parameterType="com.pojo.EnrollmentPlan">
  update enroll_plan
  <set>
    <if test="school != null">
      school = #{school},
    </if>
    <if test="testKind != null">
      testKind = #{testKind},
    </if>
    <if test="date != null">
      date = #{date},
    </if>
    <if test="age != null">
      age = #{age},
    </if>
    <if test="address != null">
      address = #{address},
    </if>
    <if test="remark != null">
      remark = #{remark},
    </if>
    <if test="enrollment != null">
      enrollment = #{enrollment},
    </if>
  </set>
  where id = #{id}
</update>
		<resultMap type="com.pojo.EnrollmentAll" id="baseResultMap">
  <id column="id" property="id"/>
  <result column="name" property="name"/>
  <result column="age" property="age"/>
  <result column="remark" property="remark"/>
<result column="address" property="address"/>
<result column="date" property="date"/>
<result column="enrollment" property="enrollment"/>
<result column="testKind" property="testKind"/>
</resultMap>

</mapper>